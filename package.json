{
	"name": "vscode-spacexexplorer",
	"displayName": "vscode-spaceXExplorer",
	"description": "fetches SpaceX launch data from the SpaceX API",
	"version": "0.0.1",
	"publisher": "lheinema",
	"engines": {
		"vscode": "^1.37.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:spacex.browser"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "spacex.browser",
				"title": "Browse SpaceX Missions"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.37.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.2",
		"vsce": "^1.66.0"
	},
	"dependencies": {
		"node-fetch": "^2.6.0"
	}
}
